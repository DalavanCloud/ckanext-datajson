{
  "validation_enabled": false,
  "catalog_headers": {
    "conformsTo": "https://project-open-data.cio.gov/v1.1/schema",
    "describedBy": "https://project-open-data.cio.gov/v1.1/schema/catalog.json",
    "@context": "https://project-open-data.cio.gov/v1.1/schema/catalog.jsonld",
    "@type": "dcat:Catalog"
  },
  "dataset_fields_map": {
    "title": {
      "field": "title"
    },
    "description": {
      "field": "notes"
    },
    "modified": {
      "field": "metadata_modified"
    },
    "accessLevel": {
      "extra": true,
      "field": "Access Level",
      "default": "public"
    },
    "identifier": {
      "field": "identifier"
    },
    "dataQuality": {
      "extra": true,
      "field": "Data Quality"
    },
    "conformsTo": {
      "extra": true,
      "field": "Conforms To"
    },
    "describedBy": {
      "extra": true,
      "field": "Described by"
    },
    "describedByType": {
      "extra": true,
      "field": "Described by Type"
    },
    "issued": {
      "extra": true,
      "field": "issued"
    },
    "landingPage": {
      "extra": true,
      "field": "landingPage"
    },
    "license": {
      "extra": false,
      "field": "license_url"
    },
    "primaryITInvestmentUII": {
      "extra": true,
      "field": "Primary It Investment Uii"
    },
    "rights": {
      "extra": false,
      "field": "rights"
    },
    "systemOfRecords": {
      "extra": true,
      "field": "System Of Records"
    },
    "spatial": {
      "extra": false,
      "field": "spatial"
    },
    "temporal": {
      "extra": true,
      "field": "Temporal"
    },
    "publisher": {
      "extra": true,
      "field": "publisher",
      "wrapper": "catalog_publisher"
    },
    "accrualPeriodicity": {
      "extra": true,
      "field": "Accrual Periodicity",
      "wrapper": "fix_accrual_periodicity"
    },
    "contactPoint": {
      "wrapper": "build_contact_point",
      "map": {
        "fn": {
          "extra": false,
          "field": "contactName"
        },
        "hasEmail": {
          "extra": false,
          "field": "contactEmail"
        }
      }
    },
    "distribution": {
      "wrapper": "generate_distribution",
      "map": {
        "accessURL": {
          "field": "url"
        },
        "mediaType": {
          "field": "mimetype"
        },
        "format": {
          "field": "format"
        },
        "title": {
          "field": "name"
        },
        "description": {
          "field": "description"
        },
        "conformsTo": {
          "field": "conformsTo"
        },
        "describedBy": {
          "field": "describedBy"
        },
        "describedByType": {
          "field": "describedByType"
        }
      }
    },
    "keyword": {
      "extra": false,
      "type": "array",
      "field": "tags",
      "array_key": "display_name"
    },
    "bureauCode": {
      "type": "array",
      "extra": false,
      "field": "bureauCode",
      "split": ",",
      "wrapper": "bureau_code"
    },
    "programCode": {
      "type": "array",
      "extra": false,
      "field": "programCode",
      "split": ","
    },
    "language": {
      "type": "array",
      "extra": true,
      "field": "Language",
      "split": ","
    },
    "references": {
      "type": "array",
      "extra": true,
      "field": "references",
      "split": ","
    },
    "theme": {
      "type": "array",
      "extra": true,
      "field": "theme",
      "split": ","
    }
  }
}
